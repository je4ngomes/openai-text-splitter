{
  "env": {
    "es2021": true
  },
  "extends": [
    "@typescript-eslint/recommended",
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "warn",
    "prettier/prettier": "warn"
  }
}
